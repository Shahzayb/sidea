{
  "name": "sidea",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"next\" \"npm run types:generate -- --watch\"",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "prisma:generate": "prisma generate",
    "prisma:introspect": "prisma introspect",
    "postprisma:introspect": "npm run prisma:generate",
    "types:generate": "graphql-codegen --config graphql/types/types-gen.yml",
    "db:start": "cd db && docker-compose up",
    "db:stop": "cd db && docker-compose down"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.7",
    "apollo-server-micro": "^2.14.1",
    "bcryptjs": "^2.4.3",
    "env-var": "^6.1.1",
    "graphql": "^15.0.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "validator": "^13.0.0"
  },
  "license": "GPL-V3",
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.1",
    "@graphql-codegen/typescript": "1.15.1",
    "@graphql-codegen/typescript-resolvers": "^1.15.1",
    "@prisma/cli": "^2.0.0-alpha.1299",
    "@types/bcryptjs": "^2.4.2",
    "@types/gravatar": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/validator": "^13.0.0",
    "concurrently": "^5.2.0",
    "typescript": "^3.9.3"
  }
}
