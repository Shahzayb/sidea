    "@prisma/client": "^2.0.0-beta.9",
    "apollo-boost": "^0.4.9",
    "apollo-server-micro": "^2.14.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "env-var": "^6.1.1",
    "formik": "^2.1.4",
    "graphql": "^15.0.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroll-hook": "^2.0.1",
    "react-quill": "^1.3.5",
    "timeago.js": "^4.0.2",
    "validator": "^13.0.0",
    "yup": "^0.29.1"
  },
