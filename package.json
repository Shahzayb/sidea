    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "license": "GPL-V3",
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.1",
    "@graphql-codegen/typescript": "1.15.1",
    "@graphql-codegen/typescript-resolvers": "^1.15.1",
    "@prisma/cli": "^2.0.0-alpha.1299",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.2.0",
    "typescript": "^3.9.3"
  }
