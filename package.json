{
  "name": "sidea",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"next\" \"npm run types:generate -- --watch\"",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "prisma:generate": "prisma generate",
    "prisma:introspect": "prisma introspect",
    "postprisma:introspect": "npm run prisma:generate",
    "types:generate": "graphql-codegen --config graphql/types-gen.yml",
    "db:start": "cd db && docker-compose up -d",
    "db:stop": "cd db && docker-compose down"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.10.2",
